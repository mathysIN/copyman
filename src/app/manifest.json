{
  "name": "Copyman",
  "short_name": "Copyman",
  "description": "Un presse papier pas sécurisé mais pratique pour mon travail",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#287d7c",
  "theme_color": "#287d7c",
  "icons": [
    {
      "src": "/web-app-manifest-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/web-app-manifest-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "video/*", "audio/*", "text/*", "application/*"]
        }
      ]
    }
  }
}
